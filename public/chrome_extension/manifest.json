{
  "name": "DataFrame: Visualize your database",
  "short_name": "DataFrame: Visualize your database",
  "version": "1.0.0.1",
  "description": "Visualize database tables and export code to relational databases or ORMs with this free, open-source tool.",
  "manifest_version": 3,
  "minimum_chrome_version": "88",
  "homepage_url": "https://dataframe.m2kdevelopments.com",
  "action": {
    "default_title": "DataFrame",
    "default_icon": {
      "16": "icon.png",
      "32": "icon.png",
      "48": "icon.png"
    }
  },
  "permissions": [],
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "content_scripts": [],
  "web_accessible_resources": [
    {
      "resources": [
        "/*"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}