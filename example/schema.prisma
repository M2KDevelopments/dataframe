model Course {
	id Int
	name String
}

model Schools {
	id String
	userId String
	userIdUsers Users @relation(fields: [userId], references: [id])
	//timestamps
	createdAt DateTime @default(now())
	updatedAt DateTime @updatedAt
}

model Trainers {
	id Int
	name String
	user String
	count String
	level String
	courseId Int
	courseIdCourse Course @relation(fields: [courseId], references: [id])
}

model Users {
	id String
	name String
	email String
}